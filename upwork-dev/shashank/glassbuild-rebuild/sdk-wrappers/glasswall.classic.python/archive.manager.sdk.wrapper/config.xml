<?xml version="1.0" encoding="UTF-8"?>
<config>
    <pdfConfig>
        <acroform>sanitise</acroform>
        <actions_all>sanitise</actions_all>
        <internal_hyperlinks>sanitise</internal_hyperlinks>
        <external_hyperlinks>sanitise</external_hyperlinks>
        <embedded_files>sanitise</embedded_files>
        <javascript>sanitise</javascript>
        <metadata>sanitise</metadata>
    </pdfConfig>
    <wordConfig>
        <embedded_files>sanitise</embedded_files>
        <internal_hyperlinks>sanitise</internal_hyperlinks>
        <external_hyperlinks>sanitise</external_hyperlinks>
        <macros>sanitise</macros>
        <metadata>sanitise</metadata>
        <review_comments>sanitise</review_comments>
    </wordConfig>
    <pptConfig>
        <embedded_files>sanitise</embedded_files>
        <internal_hyperlinks>sanitise</internal_hyperlinks>
        <external_hyperlinks>sanitise</external_hyperlinks>
        <macros>sanitise</macros>
        <metadata>sanitise</metadata>
        <review_comments>sanitise</review_comments>
    </pptConfig>
    <xlsConfig>
        <embedded_files>sanitise</embedded_files>
        <internal_hyperlinks>sanitise</internal_hyperlinks>
        <external_hyperlinks>sanitise</external_hyperlinks>
        <macros>sanitise</macros>
        <metadata>sanitise</metadata>
        <review_comments>sanitise</review_comments>
    </xlsConfig>	
    <archiveConfig defaultCompression="zip">
        <bmp>discard</bmp>
        <txt>process</txt>
        <doc>discard</doc>
        <docx>discard</docx>
        <emf>discard</emf>
        <gif>discard</gif>
        <jpeg>discard</jpeg>
        <wav>discard</wav>
        <o>discard</o>
        <elf>discard</elf>
        <pe>discard</pe>
        <mp4>discard</mp4>
        <mpg>discard</mpg>
        <pdf>discard</pdf>
        <png>discard</png>
        <ppt>discard</ppt>
        <pptx>discard</pptx>
        <tiff>discard</tiff>
        <wmf>discard</wmf>
        <xls>discard</xls>
        <xlsx>discard</xlsx>
        <mp3>discard</mp3>
    </archiveConfig>
</config>